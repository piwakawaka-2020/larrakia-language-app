{
  "name": "larrakia-language-app",
  "version": "1.0.0",
  "description": "The technologies in use:",
  "main": "server/index.js",
  "scripts": {
    "dev": "run-p watch:client watch:server",
    "watch:client": "webpack --watch",
    "watch:server": "nodemon --watch server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db-reset": "run-s db:*",
    "db:delete": "rm server/db/dev.sqlite3",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "start": "node server",
    "test": "jest --noStackTrace --watchAll",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piwakawaka-2020/larrakia-language-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/piwakawaka-2020/larrakia-language-app/issues"
  },
  "homepage": "https://github.com/piwakawaka-2020/larrakia-language-app#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/client/setup-enzyme.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "sqlite3": "^4.1.1",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "knex": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.0.3",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-confetti": "^5.0.1",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-use": "^14.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "superagent": "^5.2.2"
  }
}
